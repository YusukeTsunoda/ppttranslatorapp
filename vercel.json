{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "lib/python/**/*.py",
      "use": "@vercel/python"
    }
  ],
  "env": {
    "PYTHONPATH": "lib/python"
  },
  "functions": {
    "app/api/stripe/**/*.ts": {
      "runtime": "nodejs18"
    },
    "app/api/health/route.ts": {
      "runtime": "edge"
    }
  }
} 