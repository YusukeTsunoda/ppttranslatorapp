{
  "version": 2,
  "framework": "nextjs",
  "regions": ["hnd1"],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://${VERCEL_URL}/api"
  },
  "build": {
    "env": {
      "PRISMA_CLIENT_ENGINE_TYPE": "binary"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "app/api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
} 