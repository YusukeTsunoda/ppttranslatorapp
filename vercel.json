{
  "version": 2,
  "builds": [
    {
      "src": "python_backend/Dockerfile",
      "use": "@vercel/docker"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "python_backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "app/api/stripe/**/*.ts": {
      "runtime": "@vercel/node@2.15.10"
    },
    "app/api/health/route.ts": {
      "runtime": "@vercel/node@2.15.10"
    },
    "app/api/auth/**/*.ts": {
      "runtime": "@vercel/node@2.15.10"
    }
  }
} 