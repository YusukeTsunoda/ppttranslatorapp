{
  "name": "ppttranslatorapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint app components lib types --fix",
    "lint:fix": "eslint app components lib types --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--no-experimental-fetch jest --config=jest.config.js",
    "test:coverage": "cross-env NODE_ENV=test NODE_OPTIONS=--no-experimental-fetch jest --config=jest.config.js --coverage",
    "test:watch": "cross-env NODE_ENV=test NODE_OPTIONS=--no-experimental-fetch jest --config=jest.config.js --watch",
    "test:ci": "npm run reports:create-dirs && cross-env NODE_ENV=test NODE_OPTIONS=--no-experimental-fetch npx jest --json --outputFile=reports/jest-results.json && (npx jest-html-reporter --json=reports/jest-results.json --outputPath=reports/jest-report.html || true)",
    "test:update": "cross-env NODE_ENV=test NODE_OPTIONS=--no-experimental-fetch jest --config=jest.config.js --updateSnapshot",
    "test:report": "npm run reports:create-dirs && cross-env NODE_ENV=test NODE_OPTIONS=--no-experimental-fetch jest --config=jest.config.js --coverage --reporters=default --reporters=jest-junit",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:verify": "cypress verify",
    "cypress:report": "mkdir -p cypress/results cypress/reports && npx cypress run --reporter mochawesome --reporter-options reportDir=cypress/results,overwrite=false,html=false,json=true --spec 'cypress/e2e/**/*.cy.{js,jsx,ts,tsx}' || true",
    "cypress:generate-report": "npx mochawesome-merge cypress/results/*.json > cypress/reports/report.json && npx marge cypress/reports/report.json -f report -o cypress/reports",
    "cypress:ci": "start-server-and-test dev 3000 'cypress run --config video=true,screenshotOnRunFailure=true --reporter mochawesome'",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "node prisma/seed.mjs",
    "db:seed": "node prisma/seed.mjs",
    "db:reset": "prisma migrate reset --force",
    "test:e2e": "start-server-and-test dev 3000 'cypress run'",
    "test:e2e:watch": "jest tests/e2e --runInBand --watch",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "cypress:open": "cypress open",
    "reports:clean": "rm -rf ./reports ./coverage ./cypress/results ./cypress/reports",
    "reports:create-dirs": "mkdir -p ./reports ./cypress/results ./cypress/reports",
    "build:ci": "npm run reports:clean && npm run reports:create-dirs && npm run lint && npm run type-check && npm run test:ci && npm run build",
    "vercel:deploy": "vercel deploy --prod",
    "analyze": "ANALYZE=true next build",
    "postinstall": "prisma generate",
    "validate": "npm run lint && npm run type-check && npm run format:check",
    "batch-worker": "ts-node scripts/batch-worker.ts",
    "batch-monitor": "tsx scripts/batch-worker-monitor.tsx",
    "batch-launcher": "tsx scripts/batch-worker-launcher.tsx",
    "test:full-ci": "npm run reports:clean && npm run reports:create-dirs && npm run test:report && npm run test:e2e && npm run cypress:report && npm run cypress:generate-report"
  },
  "next": "^14.2.24",
  "next-auth": "^4.24.7",
  "pptx-parser": "^1.1.7-beta.9",
