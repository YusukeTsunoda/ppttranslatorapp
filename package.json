  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.js --coverage",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.js --watch",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.js --ci --coverage --reporters=default --reporters=jest-junit",
    "test:update": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=jest.config.js --updateSnapshot",
    "cypress": "cypress open --config-file cypress.config.js",
    "cypress:run": "cypress run --config-file cypress.config.js",
    "cypress:verify": "cypress verify",
    "cypress:report": "mkdir -p cypress/results cypress/reports && npx cypress run --config-file cypress.config.js --reporter mochawesome --reporter-options reportDir=cypress/results,overwrite=false,html=false,json=true --spec 'cypress/e2e/**/*.cy.{js,jsx,ts,tsx}' || true",
    "cypress:generate-report": "npx mochawesome-merge cypress/results/*.json > cypress/reports/report.json && npx marge cypress/reports/report.json -f report -o cypress/reports",
    "cypress:ci": "start-server-and-test dev 3000 'cypress run --config-file cypress.config.js --config video=true,screenshotOnRunFailure=true --reporter mochawesome'",
    "prisma:seed": "ts-node prisma/seed.ts",
    "db:seed": "ts-node prisma/seed.ts",
    "test:e2e": "start-server-and-test dev 3000 'cypress run --config-file cypress.config.js'",
    "test:e2e:watch": "jest tests/e2e --runInBand --watch",
    "test:all": "npm run test:coverage",
    "cypress:open": "cypress open --config-file cypress.config.js",
    "reports:clean": "rimraf ./reports ./coverage ./cypress/results ./cypress/reports",
    "reports:create-dirs": "mkdir -p ./reports ./cypress/results ./cypress/reports"
  },
